}
@keyframes searchvisible {
  0% {
    opacity: 0;
    width: 0;
  }
  100% {
    opacity: 5;
    width: 100%;
  }
}
@-webkit-keyframes searchvisible {
  0% {
    opacity: 0;
    width: 0;
  }
  100% {
    opacity: 5;
    width: 100%;
  }
}
@keyframes fadein {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes fadein {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes entrance {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes entrance {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
.search-box.search-visible {
